@model GeoDetectorWebApi.Model.GeoModel?

<h2>Editing geo data</h2>
<form asp-action="Edit" asp-controller="Geo" asp-route-id="@Model.ItemId">
    <p>
        <label asp-for="ItemId">ItemId</label><br />
        <input type="text" asp-for="ItemId" />
    </p>
    <p>
        <label asp-for="Longitude">Longitude</label><br />
        <input type="text" asp-for="Longitude" />
    </p>
    <p>
        <label asp-for="Latitude">Latitude</label><br />
        <input type="text" asp-for="Latitude" />
    </p>
    <p>
        <label asp-for="Name">Name</label><br />
        <input type="text" asp-for="Name" />
    </p>
    <p>
        <input type="submit" value="Save" />
    </p>
</form>
