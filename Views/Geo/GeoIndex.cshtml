@model IEnumerable<GeoDetectorWebApi.Model.GeoModel>
 
<h2>List of geo data</h2>

<form asp-controller="Geo" asp-action="GeoIndex">
    <p>
        Title: <input type="text" name="SearchString" />
        <input type="submit" value="Search" />
    </p>
</form>

<p><a asp-action="Create">Add Geo Data</a></p>
<table class="table">
    <tr><th>ItemId</th><th>Longitude</th><th>Latitude</th><th>Name</th></tr>
    

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ItemId</td>
            <td>@item.Longitude</td>
            <td>@item.Latitude</td>
            <td>@item.Name</td>
            <td>
                <a asp-action="Edit" asp-controller="Geo" asp-route-id="@item.ItemId">Update</a>
            </td>
            <td>
                <a asp-action="Delete" asp-controller="Geo" asp-route-id="@item.ItemId">Delete</a>
            </td>
        </tr>
    }
</table>


