@model IEnumerable<GeoDetectorMvcApp.Models.CombinedModel>
<table class="table table-striped">
    <tr>
        <td>GeoLocation</td>
        <td>Longitude</td>
        <td>Latitude</td>
        <td>Name</td>
        <td>Temperature</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GeoLocation</td>
            <td>@item.Longitude</td>
            <td>@item.Latitude</td>
            <td>@item.Name</td>
            <td>@item.Temperature</td>
        </tr>
    }
</table>